<h2><%= @stock.name    %></h2>
<h2><%= @stock.symbol  %></h2>
<h2><%= @stock.price   %></h2>
<h2><%= @stock.pe      %></h2>
<h2><%= @stock.div     %></h2>

<%= link_to "Edit Stock", edit_stock_path(@stock) %>
<%= link_to "Home", stocks_path %>
<%= link_to "Delete Stock",
  stock_path(@stock),
  :method => :delete,
  :confirm => "Hey Buddy!  Are you sure you want to delete this stock?" %>

